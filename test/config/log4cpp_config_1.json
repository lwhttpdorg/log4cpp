{
	"log-pattern": "${NM}: ${yyyy}-${MM}-${dd} ${hh}:${mm}:${ss}:${ms} [${8TH}] [${L}] -- ${msg}",
	"appenders": {
		"console": {
			"out-stream": "stdout"
		},
		"file": {
			"file-path": "log/log4cpp_config_1.log"
		},
		"socket": {
			"host": "localhost",
			"port": 9443,
			"protocol": "udp",
			"prefer-stack": "IPv6"
		}
	},
	"loggers": [
		{
			"name": "root",
			"level": "INFO",
			"appenders": [
				"console",
				"file"
			]
		},
		{
			"name": "aaa",
			"level": "INFO",
			"appenders": [
				"console",
				"socket"
			]
		},
		{
			"name": "bbb",
			"level": "WARN",
			"appenders": [
				"console",
				"file",
				"socket"
			]
		}
	]
}