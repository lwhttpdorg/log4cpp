{
	"appenders": {
		"console": {
			"out_stream": "stdout"
		},
		"file": {
			"file_path": "log/log4cpp.log"
		},
		"socket": {
			"host": "0.0.0.0",
			"port": 9443,
			"prefer-stack": "AUTO",
			"protocol": "UDP"
		}
	},
	"log_pattern": "${NM}: ${yyyy}-${MM}-${dd} ${HH}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"loggers": [
		{
			"appenders": [
				"console",
				"file"
			],
			"level": "INFO",
			"name": "hello"
		},
		{
			"appenders": [
				"console",
				"file"
			],
			"level": "WARN",
			"name": "world"
		}
	],
	"root": {
		"appenders": [
			"console",
			"file"
		],
		"level": "INFO"
	}
}
