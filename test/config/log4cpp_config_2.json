{
	"log_pattern": "${NM}: ${yyyy}-${MM}-${dd} ${h}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console": {
			"out_stream": "stdout"
		},
		"file": {
			"file_path": "log/log4cpp_config_2.log"
		},
		"socket": {
			"host": "::1",
			"port": 9443,
			"protocol": "udp",
			"prefer-stack": "auto"
		}
	},
	"root": {
		"level": "INFO",
		"appenders": [
			"console",
			"file",
			"socket"
		]
	}
}