{
	"log-pattern": "${NM}: ${yyyy}-${MM}-${dd} ${h}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console": {
			"out-stream": "stdout"
		},
		"file": {
			"file-path": "log/log4cpp_config_2.log"
		},
		"socket": {
			"host": "::1",
			"port": 9443,
			"protocol": "udp",
			"prefer-stack": "IPv6"
		}
	},
	"loggers": [
		{
			"name": "root",
			"level": "INFO",
			"appenders": [
				"console",
				"file",
				"socket"
			]
		}
	]
}