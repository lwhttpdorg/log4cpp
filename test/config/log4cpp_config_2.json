{
	"log_pattern": "${yyyy}-${MM}-${dd} ${h}:${mm}:${ss}:${ms} ${NM}: [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console": {
			"out_stream": "stdout"
		},
		"file": {
			"file_path": "log/log4cpp_config_2.log"
		},
		"tcp": {
			"local_addr": "2001:0db8:85a3:0000:0000:8a2e:0370:7334",
			"port": 9443
		},
		"udp": {
			"local_addr": "::1",
			"port": 9443
		}
	},
	"root": {
		"level": "INFO",
		"appenders": [
			"console",
			"file",
			"tcp",
			"udp"
		]
	}
}