{
	"log-pattern": "${12NM}: ${yyyy}-${MM}-${dd} ${HH}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console": {
			"out-stream": "stdout"
		},
		"file": {
			"file-path": "log/file_appender_test.log"
		}
	},
	"loggers": [
		{
			"name": "aaa",
			"level": "INFO",
			"appenders": [
				"console",
				"file"
			]
		},
		{
			"name": "bbb",
			"level": "WARN",
			"appenders": [
				"console",
				"file"
			]
		},
		{
			"name": "root",
			"level": "INFO",
			"appenders": [
				"console",
				"file"
			]
		}
	]
}