{
	"log_pattern": "${NM}: ${yy}-${MMM}-${dd} ${h}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console": {
			"out_stream": "stdout"
		},
		"file": {
			"file_path": "log_v2.log"
		},
		"tcp": {
			"local_addr": "0.0.0.0",
			"port": 9443
		},
		"udp": {
			"local_addr": "0.0.0.0",
			"port": 9443
		}
	},
	"root": {
		"level": "WARN",
		"appenders": [
			"console",
			"file"
		]
	},
	"loggers": [
		{
			"name": "aaa",
			"level": "ERROR",
			"appenders": [
				"console",
				"file"
			]
		},
		{
			"name": "ccc",
			"level": "TRACE",
			"appenders": [
				"console",
				"file"
			]
		}
	]
}