{
	"log_pattern": "${yyyy}-${MM}-${dd} ${HH}:${mm}:${ss}:${ms} ${NM}: [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console": {
			"out_stream": "stdout"
		},
		"tcp": {
			"local_addr": "0.0.0.0",
			"port": 9443
		},
		"udp": {
			"local_addr": "0.0.0.0",
			"port": 9443
		}
	},
	"loggers": [
		{
			"name": "tcp",
			"level": "INFO",
			"appenders": [
				"console",
				"tcp"
			]
		},
		{
			"name": "udp",
			"level": "INFO",
			"appenders": [
				"console",
				"udp"
			]
		}
	],
	"root": {
		"level": "WARN",
		"appenders": [
			"console",
			"tcp",
			"udp"
		]
	}
}