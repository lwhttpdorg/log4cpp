{
	"log-pattern": "${NM}: ${yyyy}-${MM}-${dd} ${HH}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${msg}",
	"appenders": {
		"console": {
			"out-stream": "stdout"
		},
		"socket": {
			"host": "localhost",
			"port": 9443,
			"protocol": "udp",
			"prefer-stack": "IPv6"
		}
	},
	"loggers": [
		{
			"name": "root",
			"level": "TRACE",
			"appenders": [
				"console",
				"socket"
			]
		}
	]
}