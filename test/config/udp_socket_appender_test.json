{
	"log_pattern": "${NM}: ${yyyy}-${MM}-${dd} ${HH}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console": {
			"out_stream": "stdout"
		},
		"socket": {
			"host": "0.0.0.0",
			"port": 9443,
			"protocol": "udp",
			"prefer-stack": "auto"
		}
	},
	"loggers": [
		{
			"name": "test",
			"level": "INFO",
			"appenders": [
				"console",
				"socket"
			]
		}
	],
	"root": {
		"level": "WARN",
		"appenders": [
			"console",
			"socket"
		]
	}
}