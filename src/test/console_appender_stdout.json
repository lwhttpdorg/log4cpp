{
	"logger_pattern": "${yyyy}-${MM}-${dd} ${HH}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console_appender": {
			"out_stream": "stdout"
		}
	},
	"loggers": [
		{
			"name": "console_logger",
			"log_level": "INFO",
			"appenders": [
				"console_appender",
				"tcp_appender",
				"udp_appender"
			]
		}
	],
	"root_logger": {
		"log_level": "INFO",
		"appenders": [
			"console_appender"
		]
	}
}
