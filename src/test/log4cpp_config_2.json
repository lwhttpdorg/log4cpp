{
	"logger_pattern": "${NM}: ${yyyy}-${MM}-${dd} ${h}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console_appender": {
			"out_stream": "stdout"
		},
		"file_appender": {
			"file_path": "log/log4cpp_config_2.log"
		},
		"tcp_appender": {
			"local_addr": "2001:0db8:85a3:0000:0000:8a2e:0370:7334",
			"port": 9443
		},
		"udp_appender": {
			"local_addr": "::1",
			"port": 9443
		}
	},
	"root_logger": {
		"log_level": "INFO",
		"appenders": [
			"console_appender",
			"file_appender",
			"tcp_appender",
			"udp_appender"
		]
	}
}