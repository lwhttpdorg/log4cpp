{
	"logger_pattern": "${NM}: ${yyyy}-${MM}-${dd} ${HH}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console_appender": {
			"out_stream": "stdout"
		},
		"file_appender": {
			"file_path": "log/file_appender_test.log"
		},
		"tcp_appender": {
			"local_addr": "0.0.0.0",
			"port": 9443
		},
		"udp_appender": {
			"local_addr": "0.0.0.0",
			"port": 9443
		}
	},
	"loggers": [
		{
			"name": "info_logger",
			"log_level": "INFO",
			"appenders": [
				"console_appender",
				"file_appender"
			]
		},
		{
			"name": "warn_logger",
			"log_level": "WARN",
			"appenders": [
				"console_appender",
				"file_appender"
			]
		}
	],
	"root_logger": {
		"log_level": "INFO",
		"appenders": [
			"console_appender",
			"file_appender"
		]
	}
}
