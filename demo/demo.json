{
	"log_pattern": "${4NM}: ${yyyy}-${MM}-${dd} ${HH}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"console": {
			"out_stream": "stdout"
		},
		"file": {
			"file_path": "log/log4cpp.log"
		},
		"socket": {
			"host": "0.0.0.0",
			"port": 9443,
			"protocol": "tcp",
			"prefer-stack": "auto"
		}
	},
	"root": {
		"level": "INFO",
		"appenders": [
			"console",
			"file"
		]
	},
	"loggers": [
		{
			"name": "hello",
			"level": "INFO",
			"appenders": [
				"console",
				"file"
			]
		},
		{
			"name": "world",
			"level": "WARN",
			"appenders": [
				"console",
				"file"
			]
		}
	]
}